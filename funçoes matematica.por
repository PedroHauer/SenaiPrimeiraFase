programa {
  funcao real multiplicacao(inteiro valor5, inteiro valor6){
    retorne valor5*valor6
  }
  
  funcao inteiro divisao(inteiro valor3, inteiro valor4){
    retorne valor3/valor4
  }



  funcao inteiro soma(inteiro valor1, inteiro valor2){
    retorne valor1 + valor2
  }


  funcao inicio() {
    escreva(soma (5,2) , ("\n"))
    escreva(soma (10,8) , ("\n"))
    escreva(divisao (18,6) , ("\n"))
    escreva(multiplicacao (17, 9))
    
  }
}
